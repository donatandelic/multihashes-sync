{
  "name": "multihashes-sync",
  "version": "0.0.2",
  "description": "Sync SHA256, SHA512 multihashes",
  "keywords": [
    "multiformats",
    "multihashes",
    "ipld",
    "sha256",
    "sha512"
  ],
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukstv/multihashes-sync.git"
  },
  "bugs": {
    "url": "https://github.com/ukstv/multihashes-sync/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "browser": "./dist/index.browser.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "test": "./node_modules/.bin/tsm ./node_modules/uvu/bin.js . .+\\.test\\.ts",
    "build": "./node_modules/.bin/tsc --project tsconfig.build.json",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "prettier": "^2.7.1",
    "tsm": "^2.2.2",
    "typescript": "^4.8.3",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "@stablelib/sha256": "^1.0.1",
    "@stablelib/sha512": "^1.0.1",
    "multiformats": "^9.9.0"
  }
}
